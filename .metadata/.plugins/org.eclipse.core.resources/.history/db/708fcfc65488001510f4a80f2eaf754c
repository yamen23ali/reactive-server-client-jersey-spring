@Component
@Path( "" )
public class StockEndpoint
{

    @Path( "/server" )
    @GET
    @Produces( "text/plain" )
    public void stockBySimpleSku(  @Suspended
    	    final AsyncResponse asyncÙˆ@QueryParam( value = "test" ) String test ) throws InterruptedException
    {
    	System.out.println( test );
    	Thread.sleep( 5000 );
        return test;
    }
}
